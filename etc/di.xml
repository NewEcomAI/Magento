        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="newecomai_shopsmart:sync_catalog_command" xsi:type="object">
                    NewEcomAI\ShopSmart\Console\Command\SyncCatalogCommand</item>
            </argument>
        </arguments>
    </type>
    <type name="NewEcomAI\ShopSmart\Console\Command\SyncCatalogCommand">
