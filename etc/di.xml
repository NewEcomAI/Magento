            <argument name="helperData" xsi:type="object">NewEcomAI\ShopSmart\Helper\Data</argument>
            <argument name="productCollectionFactory" xsi:type="object">Magento\Catalog\Model\ResourceModel\Product\CollectionFactory\Proxy</argument>
            <argument name="area" xsi:type="object">NewEcomAI\ShopSmart\Helper\Area\Proxy</argument>
        </arguments>
    </type>
</config>
