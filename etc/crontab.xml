<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="newcomai">
        <job name="newecomai_shopsmart_initial_catalog_sync" instance="NewEcomAI\ShopSmart\Cron\InitialCatalogSync"
             method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="newecomai_shopsmart_delete_upload_image_media_folder" instance="NewEcomAI\ShopSmart\Cron\DeleteUploadImageMediaFolder" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
